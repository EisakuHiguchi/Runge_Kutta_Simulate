package ex02;

public class f3 extends f{

	@Override
	public double f(double Vm, double m, double h, double n) {
		double An = 0.01*(10.0-Vm)/(Math.exp((10.0-Vm)/10.0)-1.0);
		double Bn = 0.125*Math.exp(-1*Vm/80.0);
		
		An = 0.01*(10.0-Vm)/(Math.exp((10.0-Vm)/10.0)-1.0);
		Bn = 0.125*Math.exp(-1*Vm/80.0);
		
		return An * (1.0 - z) - Bn*z;
	}
	
}
